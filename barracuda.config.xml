<!--<!DOCTYPE Barracuda SYSTEM "http://microdoc.de/dtd/barracuda.config.dtd">-->
<!DOCTYPE Barracuda [ <!ELEMENT Barracuda (Startup,Logging,Configuration*,CustomConfiguration*)> <!ATTLIST Barracuda Version CDATA #REQUIRED > <!ELEMENT Startup (Load*)> <!ELEMENT Load EMPTY> <!ATTLIST Load SCom CDATA #REQUIRED > <!ELEMENT Logging (Property)*> <!ELEMENT Configuration (Property)*> <!ATTLIST Configuration SCom CDATA #REQUIRED > <!ELEMENT Property (#PCDATA | Property)*> <!ATTLIST Property Name CDATA "" > <!ELEMENT CustomConfiguration EMPTY> <!ATTLIST CustomConfiguration Name CDATA #REQUIRED > ]>
<Barracuda Version="v1.0.0">
	<!-- The Startup-tag is used for configuring the server startup . At the
		moment there is only loading of SComs. -->
	<Startup>
		<!-- The Load-tag is used for defining the SComs which should ne loaded
			at startup. The SCom-attribute may hold the SCom name (which is returned
			by the getName() method) or its classname. -->
		<!-- <Load SCom="SessionManager" /> Not necessary since start() needs not
			be called and it will be implicitly loaded by ClientInterfaces -->
		<Load SCom="ServerProperties" />
		<Load SCom="ClientInterface" />
		<Load SCom="SessionManager" />
		<Load SCom="ProfilerManager" />
		<!-- <Load SCom="ConsoleClientInterface" /> -->
		<Load SCom="de.cerpsw.barracuda.jobserver.JobServerSCom" />
	</Startup>
	<Logging>
		<Property Name="defaultLogLevel">INFO</Property>
		<!-- defines what loglevel should be used for all undefined (not defined
			in the nonDefaults section) log components -->
		<Property Name="logWriter">
			<!-- defines the LogWriter class and an argument list to its constructor -->
			<Property Name="class">de.cerpsw.sysfunction.base.impl.LogFileWriter</Property>
			<Property Name="args">
				<Property>CATUNO.pro.log</Property>
				<Property>UTF-8</Property>
			</Property>
		</Property>
		<Property Name="logFormatter">
			<!-- defines the LogFormatter class and an argument list to its constructor -->
			<Property Name="class">com.mdsws.logging.DefaultLogFormatter</Property>
			<Property Name="args" />
		</Property>
		<Property Name="nonDefaults">
			<!-- defines all components which should not use the default log level.
				For each non-default there must be a nameless property entry, which itself
				contains the property componentId and logLevel. -->
			<Property>
				<Property Name="componentId">Session</Property>
				<Property Name="logLevel">INFO</Property>
			</Property>
		</Property>
	</Logging>
	<Configuration SCom="ServerComponentManager">
		<Property Name="SComCallTimeout">0</Property>
		<!-- defines in milliseconds how long initialization of each SCom may take,
			0 means "no limit" and should be used e.g. when debugging the startup process -->
	</Configuration>
	<Configuration SCom="ServerEnvironment">
		<!-- <Property Name="EnvironmentClass">com.mdsws.barracuda.server.tomcat.TomcatServerEnvironment</Property> -->
		<Property Name="EnvironmentClass">com.mdsws.barracuda.server.DefaultServerEnvironment</Property>
		<!-- Class that should be used for web server communincation etc.; must
			implement the ServerEnvironment interface -->
	</Configuration>
	<Configuration SCom="ClientInterface">
		<Property Name="ClientInactivityTimeout">1800000</Property>
	</Configuration>
	<Configuration SCom="KeyStore">
		<Property Name="keystore">keystore</Property> <!-- the directory in which the licence key files are stored -->
		<Property Name="customerKey">CustomerKey</Property> <!-- the name of the customer key which is used in this installation -->
	</Configuration>
	<Configuration SCom="SessionManager">
		<Property Name="MaxRecursionDepth">600</Property>
		<!-- The maximum number of recursion depth of a jpl program use -1 to switch
			off 1100 seems to be the maximum number possible under windows java with
			hotspot vm -->
		<Property Name="AutoDump">0</Property>
		<!-- Determine if a dump should be printed on every jpl runtime exception -->
		<Property Name="DebuggerPort">60606</Property>
		<!-- port used by eclipse or command line debugger to attach to the server -->
		<Property Name="LauncherPort">60607</Property> <!-- port used for starting/stopping a session or the server itself -->
		<Property Name="LogCodeCoverage">false</Property>
		<!-- if set to true barracuda code coverage log files (.bcc) will be created
			for each code file -->
		<Property Name="DebugManager">
			<!-- If this property is set to true, an automatic flush command will
				be executed each time a debugging session is suspended, i.e. all operations
				on widgets will be visible immediately. CAUTION: Currently there are some
				problems with flush during initialization of screens (setting the modality
				is not possible after the screen has been displayed). The default value is
				false, which means no flush is performed (display will be updated on explicit
				flush or when server finished processing) -->
			<Property Name="FlushOnSuspend">true</Property>
		</Property>
	</Configuration>
