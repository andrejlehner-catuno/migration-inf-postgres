	<Configuration SCom="ApplicationProperties">
		<Property Name="DefaultDateTimeFormat">dd.MM.yyyy</Property>
		<!-- e.g. used for parsing of date strings that are not in a formatted
			widget -->
		<Property Name="DecimalSymbol">.</Property>
		<!-- default decimal symbol for parsing numbers -->
		<Property Name="ThousandSep">,</Property>
		<!-- default grouping symbol, e.g. used when initially setting format_type
			to custom -->
		<Property Name="ValidationPreformatter" />
		<!-- You can insert here the class of a special handler that is called
			before a widget is validated and that may modify the widgets contents -->
		<Property Name="AutomaticLDBTransfer">false</Property>
		<!-- default is false, if set to true, sm_lstore/sm_allget will be called
			automatically during screen exit/entry -->
		<!-- Maps standard keys to userdefined keys. Property - Name to Property-Value.
			The key string must have the following syntax: <modifiers>* (<typedID> |
			<pressedReleasedID>) modifiers := shift | control | ctrl | meta | alt | altGraph
			typedID := typed <typedKey> typedKey := string of length 1 giving Unicode
			character. pressedReleasedID := (pressed | released) key key := KeyEvent
			key code name, i.e. the name following "VK_". For example:"INSERT", "control
			DELETE", "alt shift X", "alt shift released X", "typed a" for 'a' -->
		<Property Name="KeyMapping">
			<Property Name="F8">PF8</Property>
			<Property Name="ctrl F1">ITSEL</Property>
		</Property>
		<!-- Backwards mapping for KeyMapping. -->
		<Property Name="BackwardsKeyMapping">
			<Property Name="PF8">F8</Property>
			<Property Name="ITSEL">ctrl F1</Property>
			<Property Name="TAB">TAB</Property>
		</Property>
		<Property Name="GlobalControlString">
			<Property Name="alt F4">^jm_exit()</Property>
			<Property Name="ctrl alt shift F12">^openSimpleTable()</Property>
			<Property Name="ESCAPE">^sm_jclose()</Property>
		</Property>
		<Property Name="DefaultFormats">
			<Property Name="DateTime">
				<!-- Up to 10 default settings, defaults to "dd. MM. yyyy" -->
				<!-- Format String as expected by java.text.SimpleDateFormat -->
				<Property>dd.MM.yyyy HH:mm</Property>
				<!-- %0d.%0m.%4y %0h:%0M -->
				<Property>dd.MM.yyyy</Property> <!-- %0d.%0m.%4y -->
				<Property>HH:mm</Property> <!-- %0h:%0M -->
				<Property>dd.MM.yy</Property> <!-- %0d.%0m.%2y -->
				<Property>ddd/MM/yy</Property> <!-- %3d/%0m/%2y -->
				<Property>+d/yyyy</Property> <!-- %+d/%4y -->
				<Property>dd.MM.yy</Property> <!-- %0d.%0m.%2y -->
				<Property>ddMMyyyy</Property> <!-- %0d%0m%4y -->
				<Property>M/d/yy HH:mm</Property> <!-- %m/%d/%2y %h:%0M -->
				<Property>M/d/yy HH:mm</Property> <!-- %m/%d/%2y %h:%0M -->
			</Property>
			<Property Name="Numeric">
				<!-- Up to 10 default settings, see below for default values -->
				<Property>
					<!-- DEFAULT_0=lokales Währungsformat mit 2 NK -> ",22." -->
					<Property Name="Format">#,##0.00</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_1 = lokales Währungsformat ohne NK",00." -->
					<Property Name="Format">#,##0</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_2 = Mengenformat mit 2 NK -> ",22." -->
					<Property Name="Format">#,##0.00</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_3 = Mengenformat mit 3 NK -> ",33." -->
					<Property Name="Format">#,##0.000</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_4 = Mengenformat ohne NK -> ",00." -->
					<Property Name="Format">#,##0</Property> <!-- von #0. geändert da ansonsten in dem Feld ein , ensteht -wede- 02.12.2008 -->
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_5 = ganze Zahlen ohne Punkt -> ",00" -->
					<Property Name="Format">#0</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol" />
					<Property Name="ThousandSep" />
				</Property>
				<Property>
					<!-- DEFAULT_6 = Mengenformat mit 1 NK -> ",11." -->
					<Property Name="Format">#,##0.0</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_7 = Mengenformat mit 6 NK -> ",66." -->
					<Property Name="Format">#,##0.000000</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">,</Property>
					<Property Name="ThousandSep">.</Property>
				</Property>
				<Property>
					<!-- DEFAULT_8 = Mengenformat mit 2 NK -> ".22," -->
					<Property Name="Format">#,##0.00</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">.</Property>
					<Property Name="ThousandSep">,</Property>
				</Property>
				<Property>
					<!-- DEFAULT_9 = Mengenformat mit 2 NK -> ".22," -->
					<Property Name="Format">#,##0.00</Property>
					<Property Name="CurrencySymbol" />
					<Property Name="DecimalSymbol">.</Property>
					<Property Name="ThousandSep">,</Property>
				</Property>
			</Property>
		</Property>
