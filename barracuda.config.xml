	<Configuration SCom="Jpl2JavaCompiler">
		<!-- If GenerateJplBlocks is set to true, the Compiler will generate a
			java block '{' '}' for each Jpl block (not necessary, but might make code
			more recognizable) -->
		<Property Name="GenerateJplBlocks">false</Property>

		<!-- If IgnoreDeprecations is set to false, Deprecations will cause the
			compilation to fail, otherwise only a warning will be generated in the log -->
		<Property Name="IgnoreDeprecations">true</Property>
		<!-- If ExtendUsedObjects is set to true, generated code will try to use
			Value-Objects for as long as possible, if false, generated code will switch
			to Java-primitives as soon as possible (might be used to finetune generated
			code) -->
		<Property Name="ExtendUsedObjects">true</Property>
		<!-- TemporaryDirectory specifies the directory to use for temporary files
			that store javac output. If it is not set, the default Java temporary directory
			will be used -->
		<!-- <Property Name="TemporaryDirectory">tmp</Property> -->
		<!-- ChecksumMessageDigest specifies the Algorithm used by the compiler
			to generate checksums over Jpl-files (can be used by developers for an additional
			check (besides version numbering) if code has changed. The Algorithm specified
			here must be a valid parameter to java.security.MessageDigest.getInstance() -->
		<Property Name="ChecksumMessageDigest">MD5</Property>

		<!-- Enables/Disables generation of command line information in compiled
			java code for modules and screens. Default value is TRUE -->
		<Property Name="AddCommandLineInfo">true</Property>
		<!-- If this option is set to true, all keywords will be checked case insensitive
			by the compiler, if cleared, only lower case is allowed: example if set:
			FOR == For == for (keyword of for-statement) -->
		<Property Name="KeywordsCaseInsensitive">true</Property>
		<!-- This mode defines the target platform at which the code is indented
			to be executed. When ever the langauge is enhanced with new features that
			will not work in an older installation the minor number is changed. By using
			a lower compatibilty it is possible to compile for a different platform than
			is one. If the Mode is not set - the regular compilation will be used. The
			mode can be specified as an enum value as defined in com.mdsws.barracuda.server.compiler.CompatibilityMode.
			The prefix "Mode_" can be omitted, and every occurring dot (".") will be
			replaced by an underscore ("_"), so it is valid to specifiy "1.2" for Mode_1_2 -->
		<!-- UPDATE: Internal versioning of compiled classes has been introduced,
			so old compatibility mode MODE_1_2 is now version "1.0", and mode MODE_1_3
			is now version "2.0". The old descriptions can still be used and will be
			mapped to the new versioning system. -->
		<Property Name="CompatibilityMode">5.0.0</Property>

		<!-- Generated Beans are generated into this directory tree. -->
		<Property Name="GeneratedBeanDirectory" />
		<!-- Generated Beans are written into this package. -->
		<Property Name="GeneratedBeanPackage" />
		<!-- Suffix for the generated bean class name. -->
		<Property Name="GeneratedBeanSuffix">_Bean</Property>
		<!-- Generated Services are generated into this directory tree. -->
		<Property Name="GeneratedServiceDirectory" />
		<!-- The prefix that is used for fields of generated JavaBeans can be set
			with the following property. Default is no prefix. If the prefix ends with
			a letter, the first letter of the field name itself will be capitalized,
			e.g. "address" with prefix "f" => "fAddress" -->
		<Property Name="GeneratedBeanFieldPrefix">f</Property>
	</Configuration>
	<Configuration SCom="CodeCache">
		<Property Name="SourceFileEncoding">UTF-8</Property> <!-- Defines the character encoding that is used for reading JPL source files,
			default is "UTF-8" -->
		<Property Name="RecompilesAfterSystemModification">false</Property> <!-- Defines if jpl files should be recompiled after system modifications,
			default is true -->
		<Property Name="DisplayCompilationMessages">true</Property> <!-- If set to true, the compilation process will be displayed in the status
			bar, default is false -->
		<Property Name="VersionChecking">none</Property>
		<!-- Can be set to none, warning, error, default is none, on version difference
			between runtime and compiled code will log warning or fail to load with error -->
		<!-- NOTE: none is only available during initial release, future releases
			will allow only warning and error -->
	</Configuration>
	<Configuration SCom="BundleManager">
		<!-- If ErrorOnBundleNotFound is set to true, receive bundle will generate
			an error if no (named or unnamed) bundle is found. If set to false, the receive
			statement will do nothing (like in Panther) -->
		<Property Name="ErrorOnBundleNotFound">false</Property>
		<!-- DefaultMaxBundles defines the default limit of bundles that will be
			stored by the BundleManager. If more bundles than that are sent without being
			received somewhere, the oldest one(s) will be removed. -->
		<Property Name="DefaultMaxBundles">20</Property>
		<!-- If IgnoreReceiveOnReadOnlyElement is set to true, receive bundle will
			generate no error if a receive is made on an element that is readonly. Instead
			the read will be silently ignored (like in Panther). If set to false, receive
			on a read-only element will generate an error. The default value is true -->
		<Property Name="IgnoreReceiveOnReadOnlyElement">false</Property>
	</Configuration>
