	<Configuration SCom="ControlStringManager">
		<Property Name="CacheSize">1000</Property>
		<!-- Maximum number of control strings cached for all session together,
			cahced control strings are not recompiled -->
		<Property Name="DebugLevel">RUNTIME_ERROR</Property> <!-- SILENT(default), SYNTAX_ERROR,
			RUNTIME_ERROR -->
	</Configuration>
	<Configuration SCom="VariableManager">
		<Property Name="globalConstants">
			<Property Name="APP0">24578</Property>
		</Property>
	</Configuration>
	<Configuration SCom="LDBManager">
		<!-- If CompileLDBs is set to true, the ScreenCompiler is used, i.e. each
			LDB is compiled into a java class and the screen module and validation codes
			of the widgets are generated as inner classes. If CompileLDBs is false, the
			ScreenLoader is used, i.e. a LDB is directly generated from its XML description.
			Code will not be compiled (not used in LDBs) and unsed properties will be
			skipped (currently this is only the layout information) -->
		<Property Name="CompileLDBs">false</Property> <!-- default is true -->
	</Configuration>
	<Configuration SCom="ScreenManager">
		<Property Name="VersionChecking">none</Property>
		<!-- If CompileScreens is set to true, the ScreenCompiler is used, i.e.
			each screen is compiled into a java class and the screen module and validation
			codes of the widgets are generated as inner classes. If CompileScreens is
			false, the ScreenLoader is used, i.e. a screen is directly generated from
			its XML description and only code is compiled into (normal) classes named
			"<screen>_<widget>.(java|class)" -->
		<Property Name="CompileScreens">false</Property> <!-- default is true -->
		<!-- In DisplayClasses the list of possible values for the CLASS-Property
			is specified. In addition to the values here, that property also allows an
			empty value(""), which is also the default value for that property -->
		<Property Name="DisplayClasses">
			<Property>-none-</Property>
			<Property>-default-</Property>
			<Property>nur_anzeigen</Property>
			<Property>Anlage</Property>
			<Property>Daten</Property>
			<Property>change_button</Property>
			<Property>clear_button</Property>
			<Property>close_button</Property>
			<Property>continue_button</Property>
			<Property>continue_view_button</Property>
			<Property>copy_button</Property>
			<Property>delete_button</Property>
			<Property>search_view</Property>
			<Property>new_button</Property>
			<Property>non_updatable</Property>
			<Property>primary_key</Property>
			<Property>save_button</Property>
			<Property>updatable</Property>
			<Property>view_button</Property>
			<Property>view_once_button</Property>
		</Property>
		<!-- StorabeProperties defines which properties will be stored by default
			when the property state of a widget is saved. NOTE: Properties that are not
			defined for a specific widget or that are invalid (e.g. num_occurrences)
			will be silently ignored. -->
		<Property Name="StorableProperties">
			<!-- This list is meant only as an example. Note that if more properties
				are specified here, more memory is required to store the states of widgets -->
			<Property>ACTIVE</Property>
			<Property>BG_COLOR</Property>
			<Property>CLEARING_PROTECT</Property>
			<Property>FG_COLOR</Property>
			<Property>FOCUS_PROTECTION</Property>
			<Property>HIDDEN</Property>
			<Property>INPUT_PROTECTION</Property>
			<Property>MDT</Property>
			<Property>NO_VALIDATION</Property>
			<Property>REQUIRED</Property>
			<Property>SELECTED</Property>
			<Property>VALIDED</Property>
		</Property>
		<Property Name="ScreenExtensions">.xml,.bsc,.itm,.ldb,.rlb,.ldbi,.jam</Property> <!--
			List of file extensions use to open a screen -->
	</Configuration>
