				<Property Name="DEFAULT_5">DAY OF THE YEAR nur Versuch</Property>
				<Property Name="DEFAULT_6">DATE DD.MM.YY</Property>
				<Property Name="DEFAULT_7">DATE 8 INPUT</Property>
			</Property>
			<Property Name="NUMERIC">
				<Property Name="CUSTOM">Benutzerdefiniert</Property>
				<Property Name="DEFAULT_0">lokales Währungsformat mit 2 NK</Property>
				<Property Name="DEFAULT_1">lokales Währungsformat ohne NK</Property>
