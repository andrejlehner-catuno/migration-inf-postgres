			<Property Name="1">Ohne Rand</Property>
			<Property Name="0">Mit Rand</Property>
		</Property>
		<Property Name="format_type">
			<Property Name="DATE_TIME">
				<Property Name="CUSTOM">Benutzerdefiniert</Property>
				<Property Name="DEFAULT_0">DEFAULT DATE/TIME</Property>
				<Property Name="DEFAULT_1">DATE OUTPUT TT.MM.JJJJ</Property>
				<Property Name="DEFAULT_2">DEFAULT TIME</Property>
				<Property Name="DEFAULT_3">DATE 6 INPUT</Property>
